	<xs:complexType name="educationRelationType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Bildungsverhältnis</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="educationRelationId" type="eCH-0260:educationRelationIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator für Bildungsverhältnis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apprentice" type="eCH-0260:apprenticeType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Lernende Person (Personenangaben von Lernenden, Validierungskandidaten, Kandidaten nach Art. 32)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="educationDetails" type="eCH-0260:educationDetailsType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ausbildungsdetails</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractPart" type="eCH-0260:contractPartType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Teilverhältnis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="previousEducationRelationId" type="eCH-0260:educationRelationIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Vorgängiges Bildungsverhältnis (z.B. bei neuem Bildungsverhältnis nach einer Auflösung)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalEducationRelationship" type="eCH-0260:additionalEducationRelationshipType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zusätzliche Ausbildungsbeziehungen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comment" type="eCH-0260:commentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Bemerkungen</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="examAssignmentType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Prüfungszuweisung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="examAssignmentKind" type="eCH-0260:examAssignmentKindType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Art der Prüfungszuweisung Art der Prüfungszuweisung (Zuweisung setzen, Zuweisung austragen)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="examCanton" type="eCH-0007:cantonAbbreviationType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Prüfungskanton</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="examCommissionId" type="eCH-0260:examCommissionIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Durchführende Prüfungskommission</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="educationRelation" type="eCH-0260:educationRelationType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Bildungsverhältnis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="examCompany" type="eCH-0260:hostCompanyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Prüfungsbetrieb
Nicht zu liefern z. B. bei Repetenten ohne Lehrvertrag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="repetitionWithoutContract" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Wiederholung ohne Lehrvertrag
(false: nicht relevant/mit Lehrvertrag; 
true: Wiederholung ohne Lehrvertrag)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="examElementInitial" type="eCH-0260:examElementInitialType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betroffene Prüfungselemente</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="examPeriod">
				<xs:annotation>
					<xs:documentation xml:lang="de">Prüfungsperiode</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">Frühling</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">Sommer (Standardwert)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="de">Herbst</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="de">Winter</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="examYear" type="xs:gYear">
				<xs:annotation>
					<xs:documentation xml:lang="de">Prüfungsjahr im Format YYYY</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comments" type="eCH-0260:commentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Freitext-Bemerkungsfeld</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="schoolAttendanceType">
