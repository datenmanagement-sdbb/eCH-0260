					<xs:documentation xml:lang="de">Personenidentifikation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="schoolId" type="eCH-0260:schoolIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator Berufsfachschule</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="schoolYearDetails" type="eCH-0260:schoolYearDetailsType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Schuljahrdetails</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
