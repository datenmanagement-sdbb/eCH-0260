	<xs:complexType name="mutationPersonType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Mutation Personendaten</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mutationApprentice" type="eCH-0260:apprenticeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Lernende Personen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mutationVETtrainer" type="eCH-0260:VETtrainerType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Berufsbildner/innen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mutationLegalUnit" type="eCH-0260:legalUnitType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Rechtliche Einheit (Unternehmen)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mutationHostCompany" type="eCH-0260:hostCompanyType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Lehrbetrieb</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="mutationEducationRelationType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Mutation Bildungsverhältnis</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="educationRelationId" type="eCH-0260:educationRelationIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator für Bildungsverhältnis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="educationDetails" type="eCH-0260:educationDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ausbildungsdetails</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractPart" type="eCH-0260:contractPartType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Teilverhältnis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="educationMutationReason" type="eCH-0260:educationMutationReasonType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Mutationsgrund</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comment" type="eCH-0260:commentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kommentar (nur wenn Grund = Anderer Grund)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="deregistrationFromOrganisationType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Abmeldung Person an eine Organisation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="deregistrationDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Abmeldungsdatum (letzter Tag, an dem die alte Organisation gültig ist bzw. war)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deregistrationKind">
				<xs:annotation>
					<xs:documentation xml:lang="de">Art der Abmeldung</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">Grundbildung</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="de">BM</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="de">üK</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="4">
							<xs:annotation>
								<xs:documentation xml:lang="de">Prüfung</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="contractPartId" type="eCH-0260:contractPartIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator für Teilverhältnis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator der abgehenden Organisation</xs:documentation>
				</xs:annotation>
				<xs:element name="schoolId" type="eCH-0260:schoolIdType"/>
				<xs:element name="branchCoursesOrganisationId" type="eCH-0260:branchCoursesOrganisationIdType"/>
				<xs:element name="examCommissionId" type="eCH-0260:examCommissionIdType"/>
			</xs:choice>
			<xs:element name="comment" type="eCH-0260:commentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kommentar</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
