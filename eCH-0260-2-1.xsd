	<xs:complexType name="VETaccreditationType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Daten zur Bildungsbewilligung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="canton" type="eCH-0007:cantonFlAbbreviationType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Lehrortkanton</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profession" type="eCH-0260:professionType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beruf</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centrallyManaged" type="xs:boolean">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angabe, ob die Lehrstellendaten zentral auf LSR gepflegt werden (false = nein, true = ja).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="legalUnit" type="eCH-0260:legalUnitType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kontaktdaten des Unternehmens</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hostCompany" type="eCH-0260:hostCompanyType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kontaktdaten des Lehrorts</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="trialApprenticeshipOffer">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angebot an Schnupperlehren</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation>keine Schnupperlehre(n)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation>Schnupperlehre, keine Publikation</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation>Schnupperlehre mit Publikation </xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="trialApprenticeshipContact" type="eCH-0260:trialOrPreApprenticeshipContactType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kontaktdaten für Schnupperlehren</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="preApprenticeshipOffer">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angebot an Vorlehren</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1">
							<xs:annotation xml:lang="de">
								<xs:documentation>keine Vorlehre(n)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation xml:lang="de">
								<xs:documentation>Vorlehre, keine Publikation</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation xml:lang="de">
								<xs:documentation>Vorlehre mit Publikation </xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="preApprenticeshipContact" type="eCH-0260:trialOrPreApprenticeshipContactType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kontaktdaten für Vorlehren</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="options" type="eCH-0260:VETaccreditationOptionsType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Optionen zur Berufsbewilligung</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VETaccreditationAndTrainerType">
