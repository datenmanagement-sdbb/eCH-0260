	<xs:complexType name="VETaccreditationTerminationType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Auflösung Bildungsbewilligung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="hostCompanyId" type="eCH-0260:hostCompanyIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator für Lehrbetriebe</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profession" type="eCH-0260:professionType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beruf</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="terminationDate" type="xs:date" minOccurs="0">
				<xs:annotation>
